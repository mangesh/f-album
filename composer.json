{
    "name":"mangesh/f-album",
    "homepage":"https://github.com/mangesh/f-album",
    "description": "Small application to manage your facebook albums",
    "keywords": ["facebook apps", "facebook albums"],
    "type": "library",
    "license":"MIT",
    "config": {
        "vendor-dir": "lib/vendor"
    },
    "require":{
        "php":">=5.3.0",
        "slim/slim": "~2.5",
        "slim/views": "~0.1",
        "twig/twig": "~1.16",
        "panique/pdo-debug": "0.2",
        "panique/php-sass": "~1.0",
        "matthiasmullie/minify": "~1.3",
        "twbs/bootstrap": "^3.3",
        "facebook/php-sdk-v4": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.8",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload":{
        "psr-4":{
            "Mini\\": "Mini"
        }
    }
}
